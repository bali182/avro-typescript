{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "no-var-keyword": true,
    "no-console": [false],
    "interface-name": [true, "never-prefix"],
    "quotemark": [true, "single"],
    "array-type": [true, "array"],
    "semicolon": [true, "never"]
  },
  "rulesDirectory": []
}
